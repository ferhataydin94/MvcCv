@using MvcCv.Models.Entity
@model List<TblEgitimlerim>
<section class="resume-section" id="education">
    <div class="resume-section-content">
        <h2 class="mb-5">Eğitim Hayatım</h2>
       @foreach(var item in Model)
       {
        <div class="d-flex flex-column flex-md-row justify-content-between">
            <div class="flex-grow-1">
                <h3 class="mb-0">@item.AltBaslik1</h3>
                <div class="subheading mb-3">@item.AltBaslik2</div>
                <p>GPO: @item.GNO</p>
            </div>
            <div class="flex-shrink-0"><span class="text-primary">@item.Tarih</span></div>
        </div>
       }
    </div>
</section>